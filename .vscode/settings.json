{
  "cSpell.words": [
    "arbtest",
    "clippy",
    "dhat",
    "flate",
    "gmax",
    "heapless",
    "lamports",
    "Leeeon",
    "LOGSTORE",
    "napi",
    "nextest",
    "peekable",
    "Peritext",
    "RUSTFLAGS",
    "smstring",
    "thiserror",
    "txns",
    "yspan"
  ],
  "rust-analyzer.runnableEnv": {
    "RUST_BACKTRACE": "full"
  },
  "rust-analyzer.cargo.features": [
    "loro-internal/test_utils",
    "loro-internal/wasm"
  ],
  "editor.defaultFormatter": "rust-lang.rust-analyzer",
  "editor.formatOnSave": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "FUTURE",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "explorer.fileNesting.patterns": {
    "*.rs": "${capture}.excalidraw"
  },
  "excalidraw.theme": "dark",
  "deno.enable": true
}
